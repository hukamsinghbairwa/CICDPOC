<?xml version="1.0" encoding="UTF-8"?>

<FLOW VERSION="3.0" CLEANUP="true">
  <COMMENT></COMMENT>

  <!-- nodes -->

<INVOKE TIMEOUT="" SERVICE="wx.regression.schdulers.mocks:updateMocksStatus" VALIDATE-IN="$none" VALIDATE-OUT="$none">
  <COMMENT></COMMENT>
</INVOKE>

<INVOKE TIMEOUT="" SERVICE="wx.regression.dashboard:getRegressionPackState" VALIDATE-IN="$none" VALIDATE-OUT="$none">
  <COMMENT>Checking if mock enabled or not</COMMENT>

  <!-- nodes -->

<MAP MODE="OUTPUT">

  <!-- nodes -->

<MAPDELETE FIELD="/recordModeEnabled;1;0">
</MAPDELETE>
</MAP>
</INVOKE>

<BRANCH TIMEOUT="" SWITCH="/mockModeEnabled">
  <COMMENT></COMMENT>

  <!-- nodes -->

<SEQUENCE NAME="true" TIMEOUT="" EXIT-ON="FAILURE">
  <COMMENT>Mocks : Enabled</COMMENT>

  <!-- nodes -->

<INVOKE TIMEOUT="" SERVICE="wx.regression.schdulers.mocks:enableMocks" VALIDATE-IN="$none" VALIDATE-OUT="$none">
  <COMMENT></COMMENT>
</INVOKE>
</SEQUENCE>

<SEQUENCE NAME="false" TIMEOUT="" EXIT-ON="FAILURE">
  <COMMENT>Mocks : Disabled</COMMENT>

  <!-- nodes -->

<INVOKE TIMEOUT="" SERVICE="wx.regression.schdulers.mocks:clearMocks" VALIDATE-IN="$none" VALIDATE-OUT="$none">
  <COMMENT></COMMENT>
</INVOKE>
</SEQUENCE>
</BRANCH>
</FLOW>
